{
	"devDependencies": {
		"@biomejs/biome": "2.4.4",
		"@types/node": "25.3.2",
		"tsx": "4.21.0"
	},
	"name": "starter",
	"packageManager": "pnpm@10.18.1",
	"private": true,
	"scripts": {
		"build": "pnpm -r --filter './apps/*' build",
		"dev": "pnpm -r --filter './apps/*' --parallel dev",
		"format": "biome check --write .",
		"lint": "biome check .",
		"typecheck": "pnpm -r exec tsc --noEmit"
	},
	"version": "0.0.0"
}
